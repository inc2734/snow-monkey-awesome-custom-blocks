  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.25",
    "@fortawesome/react-fontawesome": "^0.1.7",
    "@inc2734/for-each-html-nodes": "^0.2.0",
    "@svgr/webpack": "^4.3.3",
    "@wordpress/scripts": "6.0.0",
    "autoprefixer": "^9.7.2",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "cpy-cli": "^2.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^6.7.1",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-wordpress": "git://github.com/WordPress-Coding-Standards/eslint-plugin-wordpress.git#1774343f6226052a46b081e01db3fca8793cc9f1",
    "hex-to-rgba": "^2.0.1",
    "masonry-layout": "^4.2.2",
    "node-sass": "^4.13.0",
    "npm-run-all": "^4.1.5",
    "po2json": "^1.0.0-alpha",
    "postcss-cli": "^6.1.3",
    "rimraf": "^3.0.0",
    "sass-basis": "^9.5.5",
    "slick-carousel": "^1.8.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "scripts": {
    "accordion:clean": "rimraf dist/block/accordion",
    "accordion:js": "wp-scripts build block/accordion/editor --output-path=dist/block/accordion",
    "accordion:css": "node-sass block/accordion -o dist/block/accordion && postcss dist/block/accordion/*.css -r",
    "accordion": "npm run accordion:clean && npm-run-all accordion:*",

    "alert:clean": "rimraf dist/block/alert",
    "alert:js": "wp-scripts build block/alert/editor --output-path=dist/block/alert",
    "alert:css": "node-sass block/alert -o dist/block/alert && postcss dist/block/alert/*.css -r",
    "alert": "npm run alert:clean && npm-run-all alert:*",

    "balloon:clean": "rimraf dist/block/balloon",
    "balloon:js": "wp-scripts build block/balloon/editor --output-path=dist/block/balloon",
    "balloon:css": "node-sass block/balloon -o dist/block/balloon && postcss dist/block/balloon/*.css -r",
    "balloon": "npm run balloon:clean && npm-run-all balloon:*",

    "box:clean": "rimraf dist/block/box",
    "box:js": "wp-scripts build block/box/editor --output-path=dist/block/box",
    "box:css": "node-sass block/box -o dist/block/box && postcss dist/block/box/*.css -r",
    "box": "npm run box:clean && npm-run-all box:*",

    "btn:clean": "rimraf dist/block/btn",
    "btn:js": "wp-scripts build block/btn/editor --output-path=dist/block/btn",
    "btn:css": "node-sass block/btn -o dist/block/btn && postcss dist/block/btn/*.css -r",
    "btn": "npm run btn:clean && npm-run-all btn:*",

    "btn-box:clean": "rimraf dist/block/btn-box",
    "btn-box:js": "wp-scripts build block/btn-box/editor --output-path=dist/block/btn-box",
    "btn-box:css": "node-sass block/btn-box -o dist/block/btn-box && postcss dist/block/btn-box/*.css -r",
    "btn-box": "npm run btn-box:clean && npm-run-all btn-box:*",

    "categories-list:clean": "rimraf dist/block/categories-list",
    "categories-list:js": "wp-scripts build block/categories-list/app block/categories-list/editor --output-path=dist/block/categories-list",
    "categories-list:css": "node-sass block/categories-list -o dist/block/categories-list && postcss dist/block/categories-list/*.css -r",
    "categories-list": "npm run categories-list:clean && npm-run-all categories-list:*",

    "child-pages:clean": "rimraf dist/block/child-pages",
    "child-pages:js": "wp-scripts build block/child-pages/nopro block/child-pages/editor --output-path=dist/block/child-pages",
    "child-pages:css": "node-sass block/child-pages -o dist/block/child-pages && postcss dist/block/child-pages/*.css -r",
    "child-pages": "npm run child-pages:clean && npm-run-all child-pages:*",

    "container:clean": "rimraf dist/block/container",
    "container:js": "wp-scripts build block/container/editor --output-path=dist/block/container",
    "container:css": "node-sass block/container -o dist/block/container && postcss dist/block/container/*.css -r",
    "container": "npm run container:clean && npm-run-all container:*",

    "contents-outline:clean": "rimraf dist/block/contents-outline",
    "contents-outline:js": "wp-scripts build block/contents-outline/nopro block/contents-outline/editor --output-path=dist/block/contents-outline",
    "contents-outline:css": "node-sass block/contents-outline -o dist/block/contents-outline && postcss dist/block/contents-outline/*.css -r",
    "contents-outline": "npm run contents-outline:clean && npm-run-all contents-outline:*",

    "countdown:clean": "rimraf dist/block/countdown",
    "countdown:js": "wp-scripts build block/countdown/app block/countdown/editor --output-path=dist/block/countdown",
    "countdown:css": "node-sass block/countdown -o dist/block/countdown && postcss dist/block/countdown/*.css -r",
    "countdown": "npm run countdown:clean && npm-run-all countdown:*",

    "directory-structure:clean": "rimraf dist/block/directory-structure",
    "directory-structure:js": "wp-scripts build block/directory-structure/editor --output-path=dist/block/directory-structure",
    "directory-structure:css": "node-sass block/directory-structure -o dist/block/directory-structure && postcss dist/block/directory-structure/*.css -r",
    "directory-structure": "npm run directory-structure:clean && npm-run-all directory-structure:*",

    "evaluation-star:clean": "rimraf dist/block/evaluation-star",
    "evaluation-star:js": "wp-scripts build block/evaluation-star/editor --output-path=dist/block/evaluation-star",
    "evaluation-star:css": "node-sass block/evaluation-star -o dist/block/evaluation-star && postcss dist/block/evaluation-star/*.css -r",
    "evaluation-star": "npm run evaluation-star:clean && npm-run-all evaluation-star:*",

    "faq:clean": "rimraf dist/block/faq",
    "faq:js": "wp-scripts build block/faq/editor --output-path=dist/block/faq",
    "faq:css": "node-sass block/faq -o dist/block/faq && postcss dist/block/faq/*.css -r",
    "faq": "npm run faq:clean && npm-run-all faq:*",

    "items:clean": "rimraf dist/block/items",
    "items:js": "wp-scripts build block/items/editor --output-path=dist/block/items",
    "items:css": "node-sass block/items -o dist/block/items && postcss dist/block/items/*.css -r",
    "items": "npm run items:clean && npm-run-all items:*",

    "limited-datetime:clean": "rimraf dist/block/limited-datetime",
    "limited-datetime:js": "wp-scripts build block/limited-datetime/editor --output-path=dist/block/limited-datetime",
    "limited-datetime:css": "node-sass block/limited-datetime -o dist/block/limited-datetime && postcss dist/block/limited-datetime/*.css -r",
    "limited-datetime": "npm run limited-datetime:clean && npm-run-all limited-datetime:*",

    "lint:js": "wp-scripts lint-js block",
    "lint": "npm-run-all -p lint:*",

    "build": "npm-run-all -p css js packages po2json img accordion alert balloon box btn btn-box categories-list child-pages container contents-outline countdown directory-structure evaluation-star faq items limited-datetime",

    "clean:css": "rimraf dist/css",
    "clean:js": "rimraf dist/js",
    "clean:packages": "rimraf dist/packages",
    "clean:zip": "rimraf snow-monkey-blocks.zip",
    "clean:img": "rimraf dist/img",
    "prepublish": "npm run build",
    "css:front:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks.scss -o dist/css",
    "css:front:postcss": "postcss dist/css/blocks.css -o dist/css/blocks.min.css -c=postcss.config.js",
    "css:front-nopro:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks-nopro.scss -o dist/css",
    "css:front-nopro:postcss": "postcss dist/css/blocks-nopro.css -o dist/css/blocks-nopro.min.css -c=postcss.config.js",
    "css:editor:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks-editor.scss -o dist/css",
    "css:editor:postcss": "postcss dist/css/blocks-editor.css -o dist/css/blocks-editor.min.css -c=postcss.config.js",
    "css:editor-nopro:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks-editor-nopro.scss -o dist/css",
    "css:editor-nopro:postcss": "postcss dist/css/blocks-editor-nopro.css -o dist/css/blocks-editor-nopro.min.css -c=postcss.config.js",
    "css:editor-wrapper:sass": "node-sass src/css/blocks-editor-wrapper.scss -o dist/css",
    "css:editor-wrapper:postcss": "postcss dist/css/blocks-editor-wrapper.css -o dist/css/blocks-editor-wrapper.min.css -c=postcss.config.js",
    "css:fallback:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/fallback.scss -o dist/css",
    "css:fallback:postcss": "postcss dist/css/fallback.css -o dist/css/fallback.min.css -c=postcss.config.js",
    "css:menu:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/menu.scss -o dist/css",
    "css:menu:postcss": "postcss dist/css/menu.css -o dist/css/menu.min.css -c=postcss.config.js",
    "css:front": "npm-run-all -s css:front:*",
    "css:front-nopro": "npm-run-all -s css:front-nopro:*",
    "css:editor": "npm-run-all -s css:editor:*",
    "css:editor-nopro": "npm-run-all -s css:editor-nopro:*",
    "css:editor-wrapper": "npm-run-all -s css:editor-wrapper:*",
    "css:fallback": "npm-run-all -s css:fallback:*",
    "css:menu": "npm-run-all -s css:menu:*",
    "css": "npm run clean:css && npm-run-all -p css:*",
    "js:front": "webpack src/js/app.js -o dist/js/app.min.js --config _webpack.config.js",
    "js:front-nopro": "webpack src/js/app-nopro.js -o dist/js/app-nopro.min.js --config _webpack.config.js",
    "js:editor": "webpack src/js/blocks.js -o dist/js/blocks-build.js --config _webpack.config.js",
    "js:editor-pro": "webpack src/js/blocks-pro.js -o dist/js/blocks-pro-build.js --config _webpack.config.js",
    "js": "npm run clean:js && npm-run-all -p js:*",
    "packages:fontawesome": "cpy 'fontawesome-free/js/all.min.js' 'fontawesome-free/LICENSE.txt' '../../dist/packages' --cwd=node_modules/@fortawesome --parents",
    "packages:slick-carousel": "cpy 'slick/**' '../../dist/packages' --cwd=node_modules/slick-carousel --parents",
    "packages": "npm run clean:packages && npm-run-all packages:*",
    "zip:pack": "rsync -a --exclude='/vendor' --exclude='/node_modules' --exclude='.*' --exclude='*.ruleset.xml' --exclude='*.config.js' --exclude='*.xml.dist' --exclude='/bin' --exclude='/tests' --exclude='package*.json' --exclude='src/img' . snow-monkey-blocks && cd snow-monkey-blocks && composer install --no-dev",
    "zip:archive": "zip -9 -qmr snow-monkey-blocks.zip snow-monkey-blocks",
    "zip": "npm run clean:zip && npm run zip:pack && npm run zip:archive",
    "po2json:snow-monkey-blocks-editor": "po2json languages/snow-monkey-blocks-ja.po languages/snow-monkey-blocks-ja-snow-monkey-blocks-editor.json -f jed",
    "po2json": "npm-run-all -p po2json:*",
    "img": "npm run clean:img && cpy '**' '../../dist/img' --cwd=src/img --parents"
  },
  "dependencies": {
    "@wordpress/i18n": "^3.7.0",
    "react-image": "2.2.1"
  }
}
