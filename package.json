{
  "author": "inc2734",
  "license": "GPL-2.0+",
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.4.5",
    "@fortawesome/fontawesome-free": "^5.8.2",
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/react-fontawesome": "^0.1.4",
    "@svgr/webpack": "^4.3.0",
    "@inc2734/for-each-html-nodes": "^0.2.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.6",
    "classnames": "^2.2.6",
    "cpy-cli": "^2.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-wordpress": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-wordpress": "git://github.com/WordPress-Coding-Standards/eslint-plugin-wordpress.git#1774343f6226052a46b081e01db3fca8793cc9f1",
    "masonry-layout": "^4.2.2",
    "node-sass": "^4.12.0",
    "npm-run-all": "^4.1.5",
    "po2json": "^1.0.0-alpha",
    "postcss-cli": "^6.1.2",
    "rimraf": "^2.6.3",
    "sass-basis": "^9.2.14",
    "slick-carousel": "^1.8.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "scripts": {
    "clean:css": "rimraf dist/css",
    "clean:js": "rimraf dist/js",
    "clean:packages": "rimraf dist/packages",
    "clean:zip": "rimraf snow-monkey-blocks.zip",
    "build": "npm-run-all -p css js packages po2json",
    "prepublish": "npm run build",
    "lint": "./node_modules/.bin/eslint .",
    "css:front:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks.scss -o dist/css",
    "css:front:postcss": "postcss dist/css/blocks.css -o dist/css/blocks.min.css -c=postcss.config.js",
    "css:front-nopro:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks-nopro.scss -o dist/css",
    "css:front-nopro:postcss": "postcss dist/css/blocks-nopro.css -o dist/css/blocks-nopro.min.css -c=postcss.config.js",
    "css:editor:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks-editor.scss -o dist/css",
    "css:editor:postcss": "postcss dist/css/blocks-editor.css -o dist/css/blocks-editor.min.css -c=postcss.config.js",
    "css:editor-nopro:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/blocks-editor-nopro.scss -o dist/css",
    "css:editor-nopro:postcss": "postcss dist/css/blocks-editor-nopro.css -o dist/css/blocks-editor-nopro.min.css -c=postcss.config.js",
    "css:editor-wrapper:sass": "node-sass src/css/blocks-editor-wrapper.scss -o dist/css",
    "css:editor-wrapper:postcss": "postcss dist/css/blocks-editor-wrapper.css -o dist/css/blocks-editor-wrapper.min.css -c=postcss.config.js",
    "css:fallback:sass": "node-sass --include-path=node_modules/node-normalize-scss src/css/fallback.scss -o dist/css",
    "css:fallback:postcss": "postcss dist/css/fallback.css -o dist/css/fallback.min.css -c=postcss.config.js",
    "css:front": "npm-run-all -s css:front:*",
    "css:front-nopro": "npm-run-all -s css:front-nopro:*",
    "css:editor": "npm-run-all -s css:editor:*",
    "css:editor-nopro": "npm-run-all -s css:editor-nopro:*",
    "css:editor-wrapper": "npm-run-all -s css:editor-wrapper:*",
    "css:fallback": "npm-run-all -s css:fallback:*",
    "css": "npm run clean:css && npm-run-all -p css:*",
    "js:front": "webpack src/js/app.js -o dist/js/app.min.js --config webpack.config.js",
    "js:front-nopro": "webpack src/js/app-nopro.js -o dist/js/app-nopro.min.js --config webpack.config.js",
    "js:editor": "webpack src/js/blocks.js -o dist/js/blocks-build.js --config webpack.config.js",
    "js:editor-pro": "webpack src/js/blocks-pro.js -o dist/js/blocks-pro-build.js --config webpack.config.js",
    "js": "npm run clean:js && npm-run-all -p js:*",
    "packages:fontawesome": "cpy 'fontawesome-free/js/**' 'fontawesome-free/LICENSE.txt' '../../dist/packages' --cwd=node_modules/@fortawesome --parents",
    "packages:slick-carousel": "cpy 'slick/**' '../../dist/packages' --cwd=node_modules/slick-carousel --parents",
    "packages": "npm run clean:packages && npm-run-all packages:*",
    "zip:pack": "rsync -a --exclude='/vendor' --exclude='/node_modules' --exclude='.*' --exclude='*.ruleset.xml' --exclude='*.config.js' --exclude='*.xml.dist' --exclude='/bin' --exclude='/tests' --exclude='package*.json' . snow-monkey-blocks && cd snow-monkey-blocks && composer install --no-dev",
    "zip:archive": "zip -9 -qmr snow-monkey-blocks.zip snow-monkey-blocks",
    "zip": "npm run clean:zip && npm run zip:pack && npm run zip:archive",
    "po2json:snow-monkey-blocks-editor": "po2json languages/snow-monkey-blocks-ja.po languages/snow-monkey-blocks-ja-snow-monkey-blocks-editor.json -f jed",
    "po2json": "npm-run-all -p po2json:*"
  },
  "dependencies": {
    "@wordpress/i18n": "^3.4.0"
  }
}
